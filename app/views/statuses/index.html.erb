<p id="notice"><%= notice %></p>

<h1>Listing Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Tokeno</th>
      <th>Exrelno</th>
      <th>Contno</th>
      <th>Custno</th>
      <th>Delito</th>
      <th>Type</th>
      <th>Size</th>
      <th>Custinc</th>
      <th>Weight</th>
      <th>Tollcharge</th>
      <th>Tokencharge</th>
      <th>Detcharge</th>
      <th>Triprent</th>
      <th>Triptotamt</th>
      <th>Drivname</th>
      <th>Driverbata</th>
      <th>Wghmntbata</th>
      <th>Custbata</th>
      <th>Pocdrivbata</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @statuses.each do |status| %>
      <tr>
        <td><%= status.tokeno %></td>
        <td><%= status.exrelno %></td>
        <td><%= status.contno %></td>
        <td><%= status.custno %></td>
        <td><%= status.delito %></td>
        <td><%= status.type %></td>
        <td><%= status.size %></td>
        <td><%= status.custinc %></td>
        <td><%= status.weight %></td>
        <td><%= status.tollcharge %></td>
        <td><%= status.tokencharge %></td>
        <td><%= status.detcharge %></td>
        <td><%= status.triprent %></td>
        <td><%= status.triptotamt %></td>
        <td><%= status.drivname %></td>
        <td><%= status.driverbata %></td>
        <td><%= status.wghmntbata %></td>
        <td><%= status.custbata %></td>
        <td><%= status.pocdrivbata %></td>
        <td><%= link_to 'Show', status %></td>
        <td><%= link_to 'Edit', edit_status_path(status) %></td>
        <td><%= link_to 'Destroy', status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Status', new_status_path %>
